{
  "name": "anhdo-cv",
  "version": "1.0.0",
  "description": "AnhDo - A web developer",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack --config webpack/webpack.dev.js --mode development",
    "build:prod": "webpack --config webpack/webpack.prod.js --mode production --sort-assets-by size",
    "start:dev": "nodemon --watch webpack/webpack.core.js --watch webpack/webpack.dev.js --exec \"webpack-dev-server --config webpack/webpack.dev.js\"",
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "devserver-firefox": "live-server --browser=firefox",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "AnhDo",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "clean-webpack-plugin": "^3.0.0",
    "concat": "^1.0.3",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.5.3",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^1.2.1",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "webpack-dev-server": "^3.11.0"
  }
}
